{
    "name": "nuxt-app",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "NUXT_BUILD_ENV=production NUXT_BUILD_SSR=0 nuxt generate --dotenv .env.production",
        "build:copy": "NUXT_BUILD_ENV=production NUXT_BUILD_SSR=0 nuxt generate --dotenv .env.production && node libs/release.mjs",
        "build:ssr": "NUXT_BUILD_ENV=production NUXT_BUILD_SSR=1 nuxt build --dotenv .env.production",
        "dev": "NUXT_BUILD_ENV=development nuxt dev --dotenv .env.development",
        "start": "NUXT_BUILD_ENV=production nuxt start --dotenv .env.production",
        "preview": "NUXT_BUILD_ENV=development nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write ."
    },
    "dependencies": {
        "@nuxt/image": "^1.8.1",
        "@nuxt/ui": "^2.20.0",
        "@nuxtjs/device": "^3.2.4",
        "@pinia/nuxt": "^0.5.5",
        "@tailwindcss/typography": "^0.5.15",
        "axios": "^1.7.9",
        "date-fns": "^4.1.0",
        "driver.js": "^1.3.1",
        "echarts": "^5.5.1",
        "moment": "^2.30.1",
        "number-precision": "^1.6.0",
        "nuxt": "^3.14.1592",
        "pinia": "^2.3.0",
        "radash": "^12.1.0",
        "recorder-core": "^1.3.24102001",
        "tailwindcss-animate": "^1.0.7",
        "v-calendar": "^3.1.2",
        "vconsole": "^3.15.1",
        "vue": "^3.5.13",
        "vue-dompurify-html": "^5.2.0",
        "vue-echarts": "^7.0.3",
        "vue-qrcode": "^2.2.2",
        "vue-router": "latest",
        "yup": "^1.6.1"
